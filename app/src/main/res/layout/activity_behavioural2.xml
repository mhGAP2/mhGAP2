<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tencho.mhgap2.Depression1"
    android:background="#18B04B">

    <ScrollView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:ignore="RtlHardcoded">


        <LinearLayout
            android:layout_width="368dp"
            android:layout_height="0dp"
            android:layout_marginBottom="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1">


            <TextView
                android:id="@+id/textView16"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Management"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="35dp"
                tools:ignore="HardcodedText,SpUsage" />

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="107dp"
                android:layout_height="97dp"
                android:layout_marginLeft="120dp"
                android:layout_weight="0.03"
                app:srcCompat="@drawable/management"
                tools:ignore="ContentDescription,InefficientWeight,RtlHardcoded" />

            <Button
                android:id="@+id/button1"
                android:layout_width="270dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="45dp"
                android:background="@drawable/depre"
                android:text="Protocol 1"
                android:textColor="#18B04B"
                android:textSize="18dp"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />

            <Button
                android:id="@+id/button2"
                android:layout_width="270dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="45dp"
                android:background="@drawable/depre"
                android:text="protocol 2"
                android:textColor="#18B04B"
                android:textSize="18dp"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />

            <Button
                android:id="@+id/button3"
                android:layout_width="270dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="45dp"
                android:background="@drawable/depre"
                android:text="PROTOCOL 3"
                android:textColor="#18B04B"
                android:textSize="18dp"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />


            <Button
                android:id="@+id/button4"
                android:layout_width="270dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="45dp"
                android:background="@drawable/depre"
                android:text="PROTOCOL 4"
                android:textColor="#18B04B"
                android:textSize="18dp"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />

            <Button
                android:id="@+id/button5"
                android:layout_width="270dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="45dp"
                android:background="@drawable/depre"
                android:text="PROTOCOL 5"
                android:textColor="#18B04B"
                android:textSize="18dp"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />

            <Button
                android:id="@+id/button6"
                android:layout_width="270dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="45dp"
                android:background="@drawable/depre"
                android:text="Protocol 6"
                android:textColor="#18B04B"
                android:textSize="18dp"
                tools:ignore="HardcodedText,RtlHardcoded,SpUsage" />


        </LinearLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>

