<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tencho.mhgap2.Oth1">
    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="368dp"
        android:layout_height="551dp"
        android:background="#ffffff"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="5dp"
            tools:ignore="RtlHardcoded">
            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Protocol 1(OTH)"
                android:textColor="#b33123"
                android:fontFamily="sans-serif-condensed"
                android:textSize="35sp"
                android:textAlignment="center"
                tools:ignore="HardcodedText" />
            <TextView
                android:id="@+id/textView2"
                android:textColor="#b33123"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                android:text="@string/oth1"
                android:textAlignment="center"
                />

            <TextView
                android:id="@+id/textView2"
                android:textColor="#361818"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                android:text="@string/oth2"
                android:textAlignment="center"
                tools:ignore="DuplicateIds,HardcodedText" />
            <TextView
                android:id="@+id/t"
                android:textColor="#b33123"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                android:text="@string/oth8"
                android:textAlignment="center"
                tools:ignore="DuplicateIds,HardcodedText" />
            <TextView
                android:id="@+id/textView2"
                android:textColor="#361818"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                android:text="@string/oth10"
                android:textAlignment="center"
                tools:ignore="DuplicateIds,HardcodedText" />
            <TextView
                android:id="@+id/t1"
                android:textColor="#b33123"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/othe"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                android:textAlignment="center"
                 />
        </LinearLayout>
    </ScrollView>



</android.support.constraint.ConstraintLayout>
