<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tencho.mhgap2.Substance_healthsec"
    android:background="#F68B1F">
    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="64dp"
        android:layout_height="83dp"
        app:srcCompat="@drawable/assesment"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="2dp"
        tools:layout_constraintLeft_creator="1"
        tools:ignore="ContentDescription"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="91dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView14" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="63dp"
        android:layout_height="71dp"
        app:srcCompat="@drawable/management"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="5dp"
        tools:ignore="ContentDescription"
        tools:layout_editor_absoluteX="20dp"
        android:layout_marginTop="69dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView12"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="63dp"
        android:layout_height="89dp"
        app:srcCompat="@drawable/followup"
        android:layout_marginStart="2dp"
        tools:ignore="ContentDescription"
        tools:layout_editor_absoluteX="20dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp" />
        <TextView
            android:id="@+id/textView7"
            android:layout_width="273dp"
            android:layout_height="35dp"
            android:text="HEALTH SECTION"
            android:textColor="#ffffff"
            android:textSize="30dp"
            android:layout_weight="0.52"
            tools:ignore="HardcodedText,InefficientWeight,SpUsage"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.802" />

        <Button
            android:id="@+id/b1"
            android:layout_width="252dp"
            android:layout_height="wrap_content"
            android:background="@drawable/depre"
            android:text="Assessment(Emergency)"
            android:textColor="#F68B1F"
            android:textSize="18dp"
            tools:ignore="HardcodedText,SpUsage"
            app:layout_constraintLeft_toRightOf="@+id/imageView14"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.111"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/b01"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintVertical_bias="0.478" />

        <Button
            android:id="@+id/b01"
            android:layout_width="245dp"
            android:layout_height="0dp"
            android:background="@drawable/depre"
            android:text="Assessment"
            android:textColor="#F68B1F"
            android:textSize="18dp"
            tools:ignore="HardcodedText,SpUsage"
            app:layout_constraintLeft_toRightOf="@+id/imageView12"
            android:layout_marginLeft="19dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="82dp"
            app:layout_constraintBottom_toTopOf="@+id/b2" />

        <Button
            android:id="@+id/b2"
            android:layout_width="241dp"
            android:layout_height="0dp"
            android:background="@drawable/depre"
            android:text="Management"
            android:textColor="#F68B1F"
            android:textSize="18dp"
            tools:ignore="HardcodedText,SpUsage"
            app:layout_constraintLeft_toRightOf="@+id/imageView13"
            android:layout_marginLeft="20dp"
            android:layout_marginBottom="64dp"
            app:layout_constraintBottom_toTopOf="@+id/b3" />

    <Button
        android:id="@+id/b3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/depre"
        android:text="Follow-up"
        android:textColor="#F68B1F"
        android:textSize="18dp"
        tools:ignore="HardcodedText,SpUsage"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="32dp"
        android:layout_marginRight="32dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="32dp"
        app:layout_constraintLeft_toLeftOf="parent" />

</android.support.constraint.ConstraintLayout>
