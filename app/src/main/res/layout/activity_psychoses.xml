<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tencho.mhgap2.psychoses"
    android:background="#223C97"
    tools:ignore="ExtraText">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:text="PSYCHOSES"
        android:textColor="#FFFFFF"
        android:textSize="30dp"
        android:id="@+id/textView"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.503"
        tools:ignore="HardcodedText,SpUsage" />
    <TextView
        android:id="@+id/t6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="13dp"
        android:text="@string/psy"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="12dp"
        tools:ignore="SpUsage"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="12dp"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        android:layout_marginTop="-164dp"
        app:layout_constraintHorizontal_bias="0.0" />
    <Button
        android:id="@+id/btnpsy"
        android:layout_width="142dp"
        android:layout_height="0dp"
        android:background="@drawable/depre"
        android:text="Begin Test"
        android:textColor="#223C97"
        android:textSize="20sp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        tools:ignore="HardcodedText,RtlHardcoded"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/t6"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />


</android.support.constraint.ConstraintLayout>
