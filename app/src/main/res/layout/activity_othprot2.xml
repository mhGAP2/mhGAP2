<?xml version="1.0" encoding="utf-8"?>
    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#ffffff"
        tools:context="com.example.tencho.mhgap2.Othprot2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="5dp"
            tools:ignore="RtlHardcoded">
            <TextView
                android:id="@+id/textView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="Protocol 2(OTH)"
                android:textColor="#b33123"
                android:textSize="30sp"
                android:fontFamily="sans-serif-condensed"
                android:textAlignment="center"
                tools:ignore="HardcodedText" />
            <TextView
                android:id="@+id/textView2"
                android:textColor="#b33123"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="OTHER SIGNIFICANT MENTAL HEALTH COMPLAINTS IN PEOPLE EXPOSED TO EXTREME STRESSORS  (e.g. physical or sexual violence, major accidents, bereavement or other major loss)"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                tools:ignore="HardcodedText" />

            <TextView
                android:id="@+id/textView2"
                android:textColor="#361818"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/oth4"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                tools:ignore="DuplicateIds,HardcodedText" />
            <TextView
                android:id="@+id/te"
                android:textColor="#361818"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/oth3"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                tools:ignore="DuplicateIds,HardcodedText" />
            <TextView
                android:id="@+id/text"
                android:textColor="#361818"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/oth7"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                tools:ignore="DuplicateIds,HardcodedText" />
            <TextView
                android:id="@+id/tex"
                android:textColor="#361818"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/oth9"
                android:fontFamily="sans-serif-condensed"
                android:textSize="16sp"
                tools:ignore="DuplicateIds,HardcodedText" />
        </LinearLayout>
    </ScrollView>
